                stop:0 rgba(255,255,255, 0), stop:1 rgba(71, 154, 199, 70));
    border: 0px solid gray;
    border-radius: 9px;
    margin-top: 1em;
}

QGroupBox::title {
    color: yellow;
    subcontrol-origin: margin;
    left: 10px;
    padding: 0 3px 0 3px;
}


