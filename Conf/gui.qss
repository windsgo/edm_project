QScrollBar:vertical
{
    width:40px;
    background:rgba(0,0,0,0);
    margin:0px,0px,0px,0px;
    padding-top:40px;
    padding-bottom:40px;
    border-radius: 5px;
}

QScrollBar::handle:vertical
{
    width:40px;
    background:qlineargradient(spread:reflect, x1:0, y1:0, x2:0.5, y2:0,
    stop:0 rgba(29,47,59, 255), stop:0.4 rgba(165, 165, 165, 255));
    border-top: 4px solid qlineargradient(spread:reflect, x1:0, y1:0, x2:0.5, y2:0,
            stop:0 rgba(29,47,59, 255), stop:0.4 rgba(165, 165, 165, 255));
    border-bottom: 4px solid qlineargradient(spread:reflect, x1:0, y1:0, x2:0.5, y2:0,
            stop:0 rgba(29,47,59, 255), stop:0.4 rgba(165, 165, 165, 255));
    min-height:30;
}

QScrollBar::add-line:vertical
{
    height:40px;
    width:40px;
    border-bottom-right-radius:5px;
    border-bottom-left-radius:15px;
    background-color:qconicalgradient(cx:0.5, cy:0.9, angle:-90,
    stop:0 rgba(29, 47, 59, 0),
    stop:0.35 rgba(165, 165, 165, 200),
    stop:0.5 rgba(29, 47, 59, 255),
    stop:0.65 rgba(165, 165, 165, 200),
    stop:1 rgba(29, 47, 59, 0));
    subcontrol-position:bottom;
}

QScrollBar::add-line:vertical:pressed
{
    height:40px;
    width:40px;
    border-bottom-right-radius:5px;
    border-bottom-left-radius:15px;
    background-color:qconicalgradient(cx:0.5, cy:1, angle:-90,
    stop:0 rgba(29, 47, 59, 0),
    stop:0.35 rgba(165, 165, 165, 200),
    stop:0.5 rgba(29, 47, 59, 255),
    stop:0.65 rgba(165, 165, 165, 200),
    stop:1 rgba(29, 47, 59, 0));
    subcontrol-position:bottom;
}

QScrollBar::sub-line:vertical
{
    height:40px;
    width:40px;
    border-top-right-radius:5px;
    border-top-left-radius:15px;
    background-color:qconicalgradient(cx:0.5, cy:0.1, angle:-90,
    stop:0 rgba(29, 47, 59, 255),
    stop:0.15 rgba(165, 165, 165, 200),
    stop:0.5 rgba(29, 47, 59, 0),
    stop:0.85 rgba(165, 165, 165, 200),
    stop:1 rgba(29, 47, 59, 255));
    subcontrol-position:top;
}

QScrollBar::sub-line:vertical:pressed
{
    height:40px;
    width:40px;
    border-top-right-radius:5px;
    border-top-left-radius:15px;
    background-color:qconicalgradient(cx:0.5, cy:0, angle:-90,
    stop:0 rgba(29, 47, 59, 255),
    stop:0.15 rgba(165, 165, 165, 200),
    stop:0.5 rgba(29, 47, 59, 0),
    stop:0.85 rgba(165, 165, 165, 200),
    stop:1 rgba(29, 47, 59, 255));
    subcontrol-position:top;
}


