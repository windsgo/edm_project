i = RS274Interpreter()

i.g54()
i.f(10000)
i.g91()
i.g00(z=100)
# i.g00(x=5000, y=5000, touch=True)

i.m02()

i.g54()
i.g90()
i.f(100000)

i.g00(x=0,y=0,z=0)

i.g00(x=12,y=13,z=14)
i.coord_set_all_zero()

i.g00(x=1)

i.m00()

def touch_z_neg(feed, target_z):
    i.f(feed).g90()
    i.g00(z=target_z, touch=True)
    i.g04(t=1)
    i.f(1000).g91()
    i.g00(z=5, m05=True)

touch_z_neg(1000, -50)
touch_z_neg(100, -50)

i.g04(t=1)
i.coord_set_z_zero()

i.m00()

i.g91()
for j in range(100):
    i.g00(x=0.001)
    i.g00(y=0.001)

i.m02()